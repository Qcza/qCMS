{
  "name": "qcms",
  "version": "1.0.0",
  "description": "simple cms",
  "scripts": {
    "start": "npm run server",
    "dev": "concurrently \"npm run sass\" \"npm run tsc:w\" \"npm run server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "server": "node server.js",
    "sass": "node-sass --source-comments app/styles/sass/ -o app/styles/",
    "preinstall":  "npm i typings -g",
    "postinstall": "concurrently \"typings install\" \"npm run sass\" \"npm run tsc\" ",
    "test": "jasmine-node --captureExceptions spec/",
    "seed": "node helpers/seed.js"
  },
  "repository": "https://github.com/Qcza/qCMS.git",
  "author": "Qcza",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "jasmine-node": "^1.14.5",
    "node-sass": "^3.8.0",
    "request": "^2.74.0",
    "sass-lint": "^1.8.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/platform-server": "^2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.2",
    "angular2-in-memory-web-api": "0.0.18",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.0",
    "express": "^4.14.0",
    "mongodb": "^2.2.5",
    "multer": "^1.2.0",
    "ng2-cookies": "^1.0.2",
    "ng2-uploader": "^1.0.0",
    "node-gyp": "^3.4.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17"
  }
}
